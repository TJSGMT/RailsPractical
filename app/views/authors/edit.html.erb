<h2>Edit Author</h2>
<%= form_for @author do |f|%>
    <%= f.label :first_name%>
    <%= f.text_field :first_name%><br>
    <%= f.label :last_name%>
    <%= f.text_field :last_name%><br>
     <%= f.label :dob%>
    <%= f.text_field :dob%><br>
     <%= f.label :email%>
    <%= f.text_field :email%><br>
    <br>

<%= f.submit%>
<%end%>